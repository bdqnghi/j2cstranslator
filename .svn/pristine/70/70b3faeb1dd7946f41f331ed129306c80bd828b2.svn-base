import com.ilog.translator.java2cs.configuration.info.PackageInfo;

public class CustomClassInfo extends ClassInfoImpl {
	/**
	 * Create a new ClassInfo with given name.
	 * 
	 * @param name
	 *            the name of this ClassInfo.
	 * @throws a
	 *             ClassNotFoundException.
	 */
	protected CustomClassInfo(MappingsInfo tInfo, IType type, boolean generics,
